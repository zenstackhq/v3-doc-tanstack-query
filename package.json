{
    "name": "my-react-app",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run start-server\" \"npm run start-client\" \"npm run preview\"",
        "generate": "zen generate --lite --schema src/zenstack/schema.zmodel",
        "start-server": "npx tsx src/server.ts",
        "start-client": "wait-on tcp:3100 && vite --port 3000",
        "preview": "wait-on tcp:3000 && open http://localhost:3000",
        "up": "npm i @zenstackhq/cli@next @zenstackhq/orm@next @zenstackhq/server@next @zenstackhq/tanstack-query@next"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.16",
        "@tanstack/react-query": "^5.90.6",
        "@zenstackhq/orm": "^3.0.0-beta.26",
        "@zenstackhq/server": "^3.0.0-beta.26",
        "@zenstackhq/tanstack-query": "^3.0.0-beta.26",
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "lorem-ipsum": "^2.0.8",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "sql.js": "^1.13.0",
        "tailwindcss": "^4.1.16"
    },
    "devDependencies": {
        "@eslint/js": "^9.36.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.5",
        "@types/node": "^24.6.0",
        "@types/react": "^19.1.16",
        "@types/react-dom": "^19.1.9",
        "@types/sql.js": "^1.4.9",
        "@vitejs/plugin-react": "^5.0.4",
        "@zenstackhq/cli": "^3.0.0-beta.26",
        "concurrently": "^9.2.1",
        "eslint": "^9.36.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.22",
        "globals": "^16.4.0",
        "tsx": "^4.20.6",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.45.0",
        "vite": "^7.1.7",
        "wait-on": "^9.0.1"
    }
}
